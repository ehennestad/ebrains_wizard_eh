    "datasetVersion": {
      "type": "object",
      "title": "",
      "required": ["experimentalApproach", "technique"],
      "properties": {
        "_experimentalApproachIntro": {
          "type": "string",
          "title": "Experimental approach*",
          "text": "Please indicate which experimental approaches best describe your data. Start typing to find relevant categories and select as many as appropriate. "
        },
        "experimentalApproach": {
          "type": "array",
          "minItems": 1,
          "maxItems": 8,          
          "title": "",
          "items": {
            "title": "",
            "type": "string",
            "controlledTerm": "experimentalApproach",
            "examples": [
            ],
            "exampleIDs": []
          }
        },
        "_preparationDesignIntro": {
          "type": "string",
          "title": "Preparation type",
          "text": "Please specify whether your data were acquired in vivo, in vitro etc. Remember to consider each of your methods and add all preparation types that apply."          
        },
        "preparationDesign": {
          "type": "array",
          "title": "",
          "items": {
            "title": "",
            "type": "string",
            "controlledTerm": "preparationType"
          }
        },
        "_techniqueIntro": {
          "type": "string",
          "title": "Technique*",
          "text": "Please select techniques describing your data acquisition and analysis. Start typing to find relevant techniques and then select from the list. If you cannot find the technique you are looking for, you can add it as a keyword below."
        },
        "technique": {
          "type": "array",
          "minItems": 1,
          "maxItems": 8,          
          "title": "",
          "items": {
            "title": "",
            "_name": "Technique",
            "type": "string",
            "controlledTerm": "technique",
            "examples": [
            ],
            "exampleIDs": []            
          }
        },
        "_studyTargetIntro": {
          "type": "string",
          "title": "Study target",
          "text": "Specify all interesting targets you had for producing this dataset. Please select first among the categories, and then choose an instance for that category."
        },
        "studyTarget": {
          "title": "",
          "type": "array",
          "items": {
            "title": "",
            "$ref": "#/definitions/studyTarget"
          }
        },
        "_keywordIntro": {
          "type": "string",
          "title": "Keywords",
          "text": "If there are additional key words you would like your dataset to be found by, please state them here."
        },
        "keyword": {
          "type": "array",
          "maxItems": 5,
          "title": "",
          "items": {
            "$ref": "#/definitions/keyword"            
          }
        }
